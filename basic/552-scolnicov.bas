10 FOR I=1 TO 2
20 RESTORE
30 FOR J=1 TO 10
40 READ A$
50 B$=""
60 FOR K=1 TO LEN(B$)
70 IF ASC(MID(A$,K,1))=35 B$=B$+CHR$(34) ELSE B$=B$+MID(A$,K,1)
80 NEXT
90 IF I=1 PRINT J*10;" ";B$ ELSE PRINT J*10+100;" DATA ";CHR$(34);B$;CHR$(34)
100 NEXT,
